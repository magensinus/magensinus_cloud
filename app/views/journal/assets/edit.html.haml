// New

.container-top
  .breadcrumbs
    .breadcrumbs-box
      .breadcrumbs-item
        = link_to journal_article_assets_path(@journal_article), class: "link medium gray" do
          .icon.left
            = image_tag "icons/chevron_left.png"
          Back
.container-middle.left
  .board-medium.altai-center
    - if params_text
      = render "text", object: @journal_asset
    - if params_image
      = render "image", object: @journal_asset
    - if params_video
      = render "video", object: @journal_asset
.container-bottom.left

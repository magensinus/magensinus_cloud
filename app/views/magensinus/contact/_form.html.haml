= form_with model: @magensinus_contact, local: true do |f|
  - if @magensinus_contact.errors.any?
    #error_explanation
      %h2= "#{pluralize(@magensinus_contact.errors.count, "error")} prohibited this magensinus_contact from being saved:"
      %ul
        - @magensinus_contact.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :meta_title
    = f.text_field :meta_title
  .field
    = f.label :meta_description
    = f.text_area :meta_description
  .field
    = f.label :meta_image_box
    = f.text_field :meta_image_box
  .field
    = f.label :meta_url
    = f.text_field :meta_url
  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :description
    = f.text_area :description
  .field
    = f.label :body
    = f.text_area :body
  .field
    = f.label :fax
    = f.text_field :fax
  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :phone
    = f.text_field :phone
  .field
    = f.label :address_one
    = f.text_field :address_one
  .field
    = f.label :address_two
    = f.text_field :address_two
  .field
    = f.label :postcode
    = f.text_field :postcode
  .field
    = f.label :city
    = f.text_field :city
  .field
    = f.label :country
    = f.text_field :country
  .actions
    = f.submit 'Save'
